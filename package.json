{
  "name": "i75league",
  "version": "1.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^16.18.101",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "framer-motion": "^12.7.4",
    "lucide-react": "^0.501.0",
    "next": "^15.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.4.0",
    "react-markdown": "^8.0.6",
    "styled-components": "^6.1.12",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "wdio:e2e": "wdio run tests/e2e/wdio/wdio.conf.ts",
    "wdio:spec": "npm run wdio:e2e -- --spec",
    "wdio:e2e:smoke": "npm run wdio:e2e -- --suite smoke",
    "wdio:e2e:full": "npm run wdio:e2e -- --suite full",
    "wdio:e2e:smoke:local": "cross-env BROWSERS=chrome,firefox BASE_URL=http://localhost:3000 HEADLESS=false npm run wdio:e2e:smoke",
    "wdio:e2e:smoke:prod": "cross-env BROWSERS=chrome,firefox BASE_URL=https://www.i75league.com HEADLESS=true npm run wdio:e2e:smoke",
    "wdio:e2e:full:prod": "cross-env BROWSERS=chrome,firefox,edge BASE_URL=https://www.i75league.com HEADLESS=true npm run wdio:e2e:full",
    "ci:wdio:e2e": "npm run wdio:e2e:full:prod",
    "allure:report": "allure serve allure-results",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:serve": "serve allure-report"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.25.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@wdio/allure-reporter": "^9.14.0",
    "@wdio/cli": "^9.14.0",
    "@wdio/local-runner": "^9.14.0",
    "@wdio/mocha-framework": "^9.14.0",
    "@wdio/spec-reporter": "^9.14.0",
    "allure-commandline": "^2.34.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.25.1",
    "eslint-config-next": "^15.3.1",
    "eslint-plugin-react": "^7.37.5",
    "expect": "^29.7.0",
    "expect-webdriverio": "^5.1.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.1",
    "postcss": "^8.5.3",
    "semantic-release": "^24.2.3",
    "serve": "^14.2.4",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "zod": "^3.24.4"
